---
import Header from './NavigationHeaderProject.astro'
import { projectsConfig, type NavigationProjects } from 'src/projectsConfig.const'

interface Props {
  project: NavigationProjects
}

const { project } = Astro.props
const { header } = projectsConfig[project]
const HeaderComp = typeof header === 'function' && header
---

{HeaderComp ? <HeaderComp /> : <Header name={header.name} link={header.link} />}
