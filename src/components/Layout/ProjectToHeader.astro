---
import Header from './menu/Header.astro'
import { projectToNavigation, type NavigationProjects } from './projectToNavigation'

interface Props {
  project: NavigationProjects
}

const { project } = Astro.props
const { header } = projectToNavigation[project]
const HeaderComp = typeof header === 'function' && header
---

{HeaderComp ? <HeaderComp /> : <Header name={header.name} link={header.link} />}
