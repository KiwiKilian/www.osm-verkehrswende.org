{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "npm run dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "update": "npx npm-check-updates -u",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/react": "^3.0.3",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/tailwind": "^5.0.2",
    "astro": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "husky": "^8.0.3"
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0"
  }
}
