{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "npm run dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "update": "npx npm-check-updates -u",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/react": "^3.0.3",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/tailwind": "^5.0.2",
    "@nanostores/react": "^0.7.1",
    "@nanostores/router": "^0.11.0",
    "@turf/turf": "^7.0.0-alpha.2",
    "astro": "^3.3.2",
    "maplibre-gl": "^3.5.1",
    "nanostores": "^0.9.4",
    "pmtiles": "^2.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.6",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0"
  }
}
